/* pages/index/index.wxss */
.silder{
  height: 350rpx;
}
.a, .b {
  width: 100%;
  height: 350rpx;
}
.center {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: wrap;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
}
.items {
  display: flex;
  flex: 33.3%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 50rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #eee;
  border-right: 1rpx solid #eee;
  font-size: 28rpx;
  color: #888;
}
.items image {
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 10rpx;
}
.bottom {
  display: flex;
}
.bottom view {
  flex: 1;
  height: 210rpx;
  padding: 10rpx;
}
.bottom view image {
  width: 100%;
  height: 100%;
}